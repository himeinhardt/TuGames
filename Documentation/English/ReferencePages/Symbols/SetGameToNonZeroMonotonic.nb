Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"A Kernel solution is: \"\>", "\[InvisibleSpace]", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"x", "[", "1", "]"}], "\[Rule]", 
